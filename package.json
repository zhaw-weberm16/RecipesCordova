{
  "name": "com.herokuapp.pathrecipes",
  "displayName": "RecipesCordova",
  "version": "1.0.0",
  "description": "Recipes Sample Application, which uses the Path-Framework and is wrapped in Apache Cordova to act as a native app.",
  "main": "index.js",
  "keywords": [
    "Recipes",
    "GUI",
    "JSON",
    "REST",
    "Rapid Application Development",
    "Rapid Application Manufacturing"
  ],
  "homepage": "https://github.com/zhaw-weberm16/RecipesCordova/",
  "bugs": {
    "url": "https://github.com/zhaw-weberm16/RecipesCordova/issues"
  },
  "author": {
    "name": "Manuel Weber"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhaw-weberm16/RecipesCordova"
  },
  "engines": {
    "node": "7.10.1",
    "npm": "4.0.5"
  },
  "scripts": {
    "ng": "ng",
    "start": "tsc --project server/tsconfig.json && concurrently \"ng serve\" \"nodemon server/server.js\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "tsc --project server/tsconfig.json && ng build"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "1.15.2",
    "cordova-android": "^7.0.0",
    "cordova-browser": "^5.0.3",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "express": "4.15.0",
    "path-framework-weberm16": "1.1.5",
    "pouchdb-adapter-memory": "6.3.4",
    "pouchdb-core": "6.3.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "4.2.5",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.60",
    "codelyzer": "2.0.0",
    "concurrently": "2.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "nodemon": "1.11.0",
    "protractor": "5.1.0",
    "ts-node": "2.0.0",
    "tslint": "4.4.2",
    "typescript": "2.1.5"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  }
}